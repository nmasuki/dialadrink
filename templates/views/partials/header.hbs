<!-- Header -->
<header id="top" class="home1_theme_header header clearfix">
    <section class="top-header">
        <div class="top-header-wrapper">
            <div class="container">
                <div class="row">
                    <div class="top-header-inner">
                        <div class="left-area hidden-xs">
                            <div class="currency_group hidden-xs currencies-switcher">
                                <a>
                                    <img src="https://res.cloudinary.com/nmasuki/image/upload/c_fit,w_207,h_50/logo.png" height="50px" alt="Dial a Drink logo">
                                    <span style="padding-left: 10px; color: white;  font-size: 18px;">
                                        <span>CALL</span>
                                        <span><strong>{{contactNumber}}</strong></span>
                                    </span>
                                </a>
                            </div>
                        </div>
                        <div class="tool right-area hidden-xs">
                            <ul class="unstyled">
                                
                                <li class="toolbar-customer my-wishlist"><a href="/">Home</a></li>
                                <li class="toolbar-customer my-wishlist"><a href="/contact-us">Contact Us</a></li>
                                <li class="toolbar-customer my-wishlist"><a href="/delivery-locations">Delivery Locations</a></li>
                                <li>
                                <div class="footer-ul">
                    <a href="https://instagram.com/dial_a_drink_kenya" title="{{{appTitle}}} Instagram"
                       class="icon-social instagram">
                         <img src="https://ik.imagekit.io/wcyvhkbdjw4r/Dial_A_Drink/instagram.png?ik-sdk-version=javascript-1.4.3&updatedAt=1656403108054" alt="Instagram" height="30px">
                      
                    </a>

                    <a href="https://www.facebook.com/drinkdeliverynairobi" title="{{{appTitle}}} Facebook"
                       class="icon-social facebook">
                            <img src="https://ik.imagekit.io/wcyvhkbdjw4r/Dial_A_Drink/facebooks.png?ik-sdk-version=javascript-1.4.3&updatedAt=1656402362067" alt="Facebook" height="30px">
                      
                    </a>
                    <a href="https://twitter.com/liqourdelivery" title="{{{appTitle}}} Twitter"
                       class="icon-social twitter">

                            <img src="https://ik.imagekit.io/wcyvhkbdjw4r/Dial_A_Drink/twitter.png?ik-sdk-version=javascript-1.4.3&updatedAt=1656402940870" alt="Twitter" height="30px">
                    </a>
                    
                     <a href="https://play.google.com/store/apps/details?id=com.dial.dialcustomer&pli=1"
                       class="icon-social youtube">
                         <img src="https://ik.imagekit.io/wcyvhkbdjw4r/Dial_A_Drink/googleplay.png?ik-sdk-version=javascript-1.4.3&updatedAt=1656407381730" alt="Youtube" height="30px">
                      
                    </a>  
                </div>
                </li>
                            </ul>
                            {{!-- <a href="http://bit.ly/2OZfVz1">
                                     <img src="https://ik.imagekit.io/wcyvhkbdjw4r/Dial_A_Drink/pl_Q8A94zAyV.webp?ik-sdk-version=javascript-1.4.3&updatedAt=1648474529710" height="50px" alt="Play Store">
                                    </a>  --}}

                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>

    <section class="main-header">
        <div class="main-header-wrapper">
            <div  class="visible-xs" style="height: 30px">
                <div class="mobile-top-navigation visible-xs" style="position: fixed;">
                    <button id="showLeftPush" class="visible-xs" style="display: none !important;"><i class="fa fa-bars fa-2x"></i></button>
                    <ul class="list-inline">
                        <li style="display:none !important;" class="is-mobile-login">
                            <div class="btn-group">
                                <div class="dropdown-toggle" data-toggle="dropdown">
                                    <i class="fa fa-user"></i>
                                </div>
                            </div>
                        </li>

                        <li class="is-mobile-currency currency_group">
                            {{!-- <img src="https://res.cloudinary.com/nmasuki/image/upload/c_fit,w_207,h_50/logo.png" height="25px" alt="Dial a Drink logo"> --}}
                           <div class="currencies-switcher">
                                <div>
                                    <img src="https://res.cloudinary.com/nmasuki/image/upload/c_fit,w_207,h_50/logo.png" height="30px" alt="Dial a Drink logo">
                                    <span style="padding-left: 10px; color: white;  font-size: 18px;">
                                         <a href="/">
                                         <NuxtLink to="/"></NuxtLink>
                                        <span><strong>DIAL A DRINK KENYA</strong></span>
                                        </a>
                                        
                                    </span>

                                    {{!-- <a href="https://instagram.com/dial_a_drink_kenya" title="{{{appTitle}}} Instagram" class="icon-social instagram">
                                        <img src="https://ik.imagekit.io/wcyvhkbdjw4r/Dial_A_Drink/instagram.png?ik-sdk-version=javascript-1.4.3&updatedAt=1656403108054" alt="Instagram" height="30px">
                                    </a> --}}

                                    {{!-- <a href="https://play.google.com/store/apps/details?id=com.dial.dialcustomer&pli=1">
                                        <img src="https://ik.imagekit.io/wcyvhkbdjw4r/Dial_A_Drink/googleplay.png?ik-sdk-version=javascript-1.4.3&updatedAt=1656407381730" alt="Youtube" height="30px">
                                    </a>   --}}
                                </div>
                            </div> 
                        </li>
 
                        <li style="float: right" class="is-mobile-cart">
                            <a href="/cart" title="Shopping Cart">
                                <div class="num-items-in-cart" style="display: none;">
                                    <i  style="color: white" class="fa fa-shopping-cart"></i>
                                    <span class="icon">
                                        <span  style="color: white" class="number cart-total-pieces">{{cartItemCount this}}</span>
                                    </span>
                                    <div class="ajax-subtotal" style="display:none;"></div>
                                </div>
                            </a>
                        </li>
                    </ul>
                </div>
                                       
                <div class="is-mobile-nav-menu nav-menu visible-xs" 
                    style="position: fixed;top: 36px;z-index: 100;opacity: 0.8;" id="is-mobile-nav-menu">
                    <ul class="nav navbar-nav hoverMenuWrapper">
                        <li class="nav-item">
                             
                             <NuxtLink to="/giftpack">Gift Pack</NuxtLink>
                        </li>

                        <li class="nav-item">
                            <a href="/">
                                <span>Home</span>
                            </a>
                        </li>

                        {{#each navLinks}}
                            <li class="nav-item">
                                <a href="{{{href}}}">
                                    <span>{{{label}}}</span>
                                </a>
                            </li>
                        {{/each}}
                    </ul>
                </div>
            </div>
            <div class="container clearfix">
                <div class="row">

                <div class="nav-search" style="margin-top: {{#if isMobile}}30px;{{else}}10px{{/if}} !important">
                <form class="search" action="/search" style="colo">
                    <input type="text" name="query" style="color: #000; width:300px; border: 3px solid #555; size: 20px; " class="search_box" placeholder="Search Products" value="" id="search_box"/>
                </form>
                </div>
                    <div class="main-header-inner">
                        <div class="nav-top">
                         
                            <div class="nav-logo" {{#if isMobile}}style="display:none; color: #0a0808"{{/if}}>
                                <h style="font-size: 28px;">
                                    <a href="/">
                                        <strong><span style=" color: #0a0808">{{appTitle}}</span></strong>
                                    </a>
                                </h>
                            </div>

                            <div id="cart-target" class="nav-cart ">
                                <div class="cart-info-group">
                                    <a href="/cart" class="cart dropdown-toggle dropdown-link" data-toggle="dropdown"
                                       title="Shopping Cart">
                                        <i class="sub-dropdown1 visible-sm visible-md visible-lg"></i>
                                        <i class="sub-dropdown visible-sm visible-md visible-lg"></i>
                                        <div class="num-items-in-cart">
                                            <img class="cart_img" src="/assets/carts.png" alt="My Cart"/>
                                            <span class="cart_text">
												<a href="/cart">
                                                    <span class="number cart-total-pieces" id="number">
                                                        {{cartItemCount this}}
                                                    </span>
												</a>											  
											</span>
                                        </div>
                                    </a>
                                    <div id="cart-info" class="dropdown-menu">
                                        <div id="cart-content">
                                            <div class="loading">
                                                <img src="/assets/loader.gif" alt="Loading.."/> Loading..
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="navigation_area">
                            <ul class="navigation_links">

                        <div class="col-sm-14" style="font-size: 16px; text-transform:uppercase; font-family: monospace; font-weight:bold">
                        {{#each navLinks}} 
                        <li style="display:inline"class="nav-item {{#ifgt submenus.length 0}}dropdown{{/ifgt}}">                           
                             <a href="{{{href}}}"{{#ifgt submenus.length 0}} data-toggle="dropdown"
                                           class="dropdown-toggle" {{/ifgt}}>
                                            <span>{{{label}}}</span>
                            </a>
                            {{#ifgt submenus.length 0}}
                                <ul class="dropdown-menu">
                                    {{#each submenus}}
                                        <li><a href="{{{href}}}">{{{label}}}</a></li>
                                    {{/each}}
                                </ul>
                            {{/ifgt}}
                            </li>
                        {{/each}}
                        </div>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
</header>